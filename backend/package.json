{
  "name": "mator-life-backend",
  "version": "1.0.0",
  "description": "Backend for Mator Life Management System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop mator-life-backend",
    "pm2:restart": "pm2 restart mator-life-backend",
    "pm2:reload": "pm2 reload mator-life-backend",
    "pm2:delete": "pm2 delete mator-life-backend",
    "pm2:logs": "pm2 logs mator-life-backend",
    "pm2:monit": "pm2 monit",
    "create-subscription": "ts-node src/scripts/createSubscription.ts",
    "update-master": "ts-node src/scripts/updateMasterUser.ts",
    "clear-spare-parts": "ts-node src/scripts/clearSpareParts.ts",
    "seed-spare-parts": "ts-node src/scripts/seedSpareParts.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "groq-sdk": "^0.37.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.67.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.13",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
